<h1>Sign Up</h1>

<%= form_for(@user, url:'/signup') do |f| %>

  <%= f.label :username %><br>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label :email %><br>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  <br><br>
  <%= f.submit "Submit" %>

<% end %>